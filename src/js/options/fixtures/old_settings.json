{
  ".local": {
    "title": "[DEV] {title}"
  },
  "domain.com": {
    "title": "[PROD] {title}"
  },
  "youtube.com": {
    "icon": "https://www.google.com/favicon.ico",
    "pinned": true
  },
  "twitter.com": {
    "title": "I'm working hard!",
    "icon": "{default}",
    "protected": true
  },
  "mail.google.com": {
    "unique": true
  },
  "pinterest.com/search": {
    "title": "$1 | Pinterest",
    "url_matcher": "q=([^&]+)"
  },
  "github.com": {
    "title": "{title} | $2 by $1",
    "url_matcher": "github[.]com/([A-Za-z0-9_-]+)/([A-Za-z0-9_-]+)"
  }
}
